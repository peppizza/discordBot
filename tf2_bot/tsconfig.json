{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ESNext",
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
    }
}